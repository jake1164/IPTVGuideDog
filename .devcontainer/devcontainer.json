{
  "name": "IPTVGuideDog Dev",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "workspaceFolder": "/workspaces/IPTVGuideDog",
  "runArgs": [
    "--init"
  ],
  "forwardPorts": [
    5283,
    8080
  ],
  "postCreateCommand": "dotnet restore src/IPTVGuideDog.Web/IPTVGuideDog.Web.csproj && dotnet restore src/IPTVGuideDog.Cli/IPTVGuideDog.Cli.csproj",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-dotnettools.csdevkit",
        "ms-azuretools.vscode-docker"
      ]
    }
  }
}
