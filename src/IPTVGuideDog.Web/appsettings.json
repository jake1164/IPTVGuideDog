{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=Data/app.db;Cache=Shared"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "IPTVGuideDog": {
    "Refresh": {
      "IntervalHours": 4,
      "TimeoutMinutes": 5,
      "StartupDelaySeconds": 30
    },
    "Snapshot": {
      "RetentionCount": 3
    }
  }
}
